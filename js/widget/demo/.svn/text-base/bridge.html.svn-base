		<title>DEMO</title>
		<style>
			.form{
				background-color: yellow;
				width: 400px;
			}
			.button{
				vertical-align:middle;
				text-align:center;
				font-size:12px;
				color:#FFFFFF;
				width: 100px;
				height: 20px;
				line-height:20px;
				border:1px #990 solid;
				background-color:#ADADAD;
				display:block;
			}
			.disabled{
				background-color:#EBEBEB;
			}
			.error{
				color:#ffffff;
			}
			.checkbox{
				border:#fff solid 1px;
				background-color:#EBEBEB;
				width : 10px;
				height: 10px;
				line-height: 10px;
			}
			.checkbox1{
				border:#ebebeb solid 1px;
				background-color:#990;
				width : 10px;
				height: 10px;
				line-height: 10px;
			}
			.dd{
				background-color:#990000;
			}
			.enabled{
				background-color:#990000;
			}
			.disabledClass{
				background-color:#ebebeb;
				color:#990000;
				vertical-align:middle;
				text-align:center;
				font-size:12px;
				width: 100px;
				height: 20px;
				line-height:20px;
				border:1px #990 solid;
				background-color:#ADADAD;
				display:block;
			}
			.preview{border:1px solid red;padding:10px;}
