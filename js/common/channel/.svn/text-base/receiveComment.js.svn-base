$Import('common.listener');
STK.register('common.channel.receiveComment', function($){
	var eventList = [
		//用来告诉收到的评论，要显示出来评论的隐私设置弹层
		'showPrivateSet'
	];
	return $.common.listener.define('common.channel.receiveComment', eventList);
});

