$Import('kit.io.inter');
STK.register('common.trans.topic',function($){
	var t = $.kit.io.inter();
	var g = t.register;
	g('add', {'url': '/aj/topic/add', 'method': 'post'});//pars:name
	g('del', {'url': '/aj/topic/del', 'method': 'post'});//pars:id
	g('more', {'url': '/aj/topic/get ', 'method': 'post'});//pars:uid
	return t;
});

